{
  "name": "openai-proxy-server",
  "version": "1.0.0",
  "description": "OpenAI-compatible API proxy server for Coze services",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec tsx src/server.ts",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "test": "tsx examples/test-client.ts",
    "test:raw": "tsx examples/raw-http-test.ts"
  },
  "keywords": [
    "openai",
    "coze",
    "api",
    "proxy",
    "server",
    "rest",
    "compatible"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@coze/api": "^1.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.0.0",
    "uuid": "^9.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "nodemon": "^3.0.0",
    "openai": "^4.0.0"
  }
}
